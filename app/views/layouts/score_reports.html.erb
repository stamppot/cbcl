<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN" "http://www.w3.org/TR/REC-html40/loose.dtd">
<html>
<head>
	<title><%= @page_title %></title>
	<%= javascript_include_tag 'prototype', :cache => true %>
	<%= javascript_include_tag 'scriptaculous', :cache => true %>
	<%= javascript_include_tag 'effects', :cache => true %>
	<%= javascript_include_tag 'controls', :cache => true %>
	<%= javascript_include_tag 'tabbing', :cache => true %>
  
	<%= stylesheet_link_tag 'score_report' %>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
</head>

<body>
	<% render :partial => 'layouts/info_header' %>

<div id="score_report">
	<%= render :partial => 'layouts/error_messages' %>

	<%= @content_for_layout %>
</div>

</body>
</html>
